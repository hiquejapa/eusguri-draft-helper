# Copy to .env and fill values
RIOT_API_KEY=YOUR_RIOT_API_KEY_HERE

# Comma-separated routing platforms for Match-V5 (e.g., americas,europe,asia)
PLATFORMS=americas,europe,asia

# Comma-separated regions for Summoner-V4/Match requests (e.g., br1,euw1,na1)
REGIONS=br1

# Window in days for analysis
WINDOW_DAYS=30

# Output directory
OUTPUT_DIR=out
